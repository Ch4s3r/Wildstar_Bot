        <Label x:Name="lblProcess" Content="Label" HorizontalAlignment="Left" Margin="111,7,0,0" VerticalAlignment="Top" Width="64"/>
        <Button x:Name="btnSend" Content="Send" HorizontalAlignment="Left" Margin="10,56,0,0" VerticalAlignment="Top" Width="75" Click="btnSend_Click"/>
        <TextBox x:Name="txtToSend" HorizontalAlignment="Left" Height="23" Margin="102,53,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
